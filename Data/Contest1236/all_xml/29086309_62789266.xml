// a^x ã mod ã§æ±ãã
// åå¾©äºä¹æ³
// O( log x )
long long mod_pow( long long a, long long x, long long mod )
{
	a %= mod;

	long long res = 1;
	for ( ; x; x &gt;&gt;= 1, ( a *= a ) %= mod )
	{
		if ( x &amp; 1 )
		{
			( res *= a ) %= mod;
		}
	}
	return res;
}

